{
    "name": "MyOshi",
    "description": "MyOshi takes you from Vtuber's page to his/her membership-only video list.",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [{
        "matches": ["*://*.youtube.com/*"],
        "js": ["contentScript.js"]
    }],
    "permissions": [ 
        "tabs",
        "activeTab",
        "scripting"
     ],
     "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/assets/shinomiya-16.jpeg",
            "32": "/assets/shinomiya-32.jpeg",
            "48": "/assets/shinomiya-48.jpeg",
            "128": "/assets/shinomiya-128.jpeg"
          }
        },
    "icons": {
        "16": "/assets/shinomiya-16.jpeg",
        "32": "/assets/shinomiya-32.jpeg",
        "48": "/assets/shinomiya-48.jpeg",
        "128": "/assets/shinomiya-128.jpeg"
    }
}